{
  "name": "chatroom-server",
  "version": "0.0.1",
  "author": "",
  "homepage": "",
  "private": true,
  "main": "src/index/index.js",
  "config": {
    "ionic_webpack": "./config/webpack.config.js",
    "ionic_watch": "./config/watch.config.js"
  },
  "scripts": {
    "start": "concurrently \"ionic-app-scripts serve --nobrowser\" \"wait-on http-get://localhost:8100&&export NODE_DEV=development;electron ./electron/\"",
    "ionic:serve": "ionic-app-scripts serve --nobrowser",
    "ionic:clean": "ionic-app-scripts clean",
    "ionic:build": "ionic-app-scripts build",
    "ionic:build:prod": "ionic-app-scripts build --prod --minifyjs  --generateSourceMap false",
    "ionic:lint": "ionic-app-scripts lint",
    "rebuild:dev:ia32": "electron-rebuild ./ --arch=ia32",
    "rebuild:dev:x64": "electron-rebuild ./ --arch=x64",
    "electron:init": "cd ./electron/ && npm install && cd ..",
    "electron:serve:osx": "export NODE_DEV=development;electron ./electron/",
    "electron:serve:win": "set NODE_DEV=development&&electron ./electron/",
    "electron:rebuild:ia32": "electron-rebuild ./electron/ --arch=ia32",
    "electron:rebuild:x64": "electron-rebuild ./electron/ --arch=x64",
    "electron:pack:osx": "electron-packager ./electron/ --out=dist --platform=darwin --arch=x64 --overwrite",
    "electron:pack:win32": "electron-packager ./electron/ --out=dist --platform=win32 --arch=ia32 --overwrite",
    "electron:pack:win64": "electron-packager ./electron/ --out=dist --platform=win32 --arch=x64 --overwrite",
    "electron:publish:osx": "npm run electron:rebuild:x64 && npm run electron:pack:osx",
    "electron:publish:win32": "npm run electron:rebuild:ia32 && npm run electron:pack:win32",
    "electron:publish:win64": "npm run electron:rebuild:x64 && npm run electron:pack:win64"
  },
  "dependencies": {
    "@angular/animations": "5.2.11",
    "@angular/common": "5.2.11",
    "@angular/compiler": "5.2.11",
    "@angular/compiler-cli": "5.2.11",
    "@angular/core": "5.2.11",
    "@angular/forms": "5.2.11",
    "@angular/http": "5.2.11",
    "@angular/platform-browser": "5.2.11",
    "@angular/platform-browser-dynamic": "5.2.11",
    "@capacitor/cli": "^1.0.0-beta.7",
    "@ionic/core": "^4.0.0-beta.3",
    "@ionic/storage": "2.1.3",
    "@jimp/gif": "^0.5.0",
    "better-sqlite3": "^4.1.4",
    "blueimp-md5": "^2.10.0",
    "clipboard": "^2.0.1",
    "common.communicate": "file:../common/communicate",
    "common.download": "file:../common/download",
    "common.messages": "file:../common/messages",
    "common.utils": "file:../common/utils",
    "crypto-js": "^3.1.9-1",
    "ionic-angular": "3.9.2",
    "ionicons": "3.0.0",
    "jimp": "^0.5.3",
    "js-base64": "^2.4.9",
    "math": "0.0.3",
    "microtime": "^2.1.8",
    "nkn-client": "^0.4.4",
    "qrious": "^4.0.2",
    "rxjs": "5.5.11",
    "shelljs": "^0.8.2",
    "sw-toolbox": "3.6.0",
    "wait-on": "^3.1.0",
    "zone.js": "0.8.26"
  },
  "devDependencies": {
    "@ionic/app-scripts": "3.1.11",
    "electron": "^2.0.8",
    "electron-packager": "^12.1.1",
    "electron-rebuild": "^1.8.2",
    "typescript": "~2.6.2"
  },
  "description": "An Ionic project"
}
